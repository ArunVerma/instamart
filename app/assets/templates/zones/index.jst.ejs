<h1>Listing <%= collection.url %></h1>

<table>
  <thead>
    <tr>
      <% collection.first().keys().each(function (key) { %>
        <th><%= key %></th>
      <% }); %>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% collection.each(function (model) { %>
      <tr>
        <% model.keys().each(function (key) { %>
          <td><%= model.escape(key) %></td>
          <td><a href="#/<%= collection.url %>/<%= model.id %>">Show</a></td>
          <td><a href="#/<%= collection.url %>/<%= model.id %>/edit">Edit</a></td>
          <td><a class="delete" data-id="<%= model.id %>">Delete</a></td>
        <% }); %>
      </tr>
      <% }); %>
    </tbody>
  </table>

  <br>

  <a href="#/<%= collection.url %>/new">New</a>
