<div class="headline">
  <button class="close">Ã—</button>
  <h2 class="muted">
    Whole Foods Market
    <a class="btn-change-warehouse btn-green btn-hollow" href="javaScript:void(0);">Change Store</a>
  </h2>
</div>
<div class="departments">
  <div class="department-list featured-departments">
    <a class="department popular nav-tab" href="#whole-foods" data-department-id="popular">
      <span class="department-icon">
        <i class="icon-bookmark"></i>
      </span>
      Popular
    </a>
    <a class="department lists nav-tab" href="#whole-foods/lists" data-department-id="lists">
      <span class="department-icon">
        <i class="icon-list"></i>
      </span>
      Lists &amp; Recipes
    </a>
    <a class="department favorites nav-tab" href="#whole-foods/favorites" data-department-id="favorites">
      <span class="department-icon">
        <i class="icon-star"></i>
      </span>
      Favorites
    </a>
  </div>
  <div class="department-list in-columns">
    <% departments.each(function (department) { %>

    <a class="department" href="#whole-foods/departments/<%= department.id %>" data-department-id="<%= department.id %>">
      <%= department.escape('name') %>
    </a>

    <% }); %>
  </div>
</div>
