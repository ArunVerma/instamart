<h1>Listing stores</h1>

<table>
  <thead>
    <tr>
      <th>Zone</th>
      <th>Name</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Img url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% stores.each(function (store) { %>
    <tr>
      <td><%= store.escape('zone_id') %></td>
      <td><%= store.escape('name') %></td>
      <td><%= store.escape('street') %></td>
      <td><%= store.escape('city') %></td>
      <td><%= store.escape('state') %></td>
      <td><%= store.escape('zipcode') %></td>
      <td><%= store.escape('img_url') %></td>
      <td><a href="#/zones/<%= store.escape('zone_id') %>stores/<%= store.id %>">Show</a></td>
      <td><a href="#/zones/<%= store.escape('zone_id') %>stores/<%= store.id %>/edit">Edit</a></td>
      <td><a class="delete" data-id="<%= store.id %>">Delete</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<br>

<a href="#/zones/<%= store.escape('zone_id') %>stores/new">New</a>
