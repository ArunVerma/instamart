</div></div>
<div class="favorites content-panel"></div>
<div class="department content-panel active">
  <div class="items-board-container popular">
    <div class="infinite-scrolling items-board unstyled">
      <% store.depts().each(function (dept) { %>
        <div class="item-header">
          <h4><a href="#zones/1/stores/1/departments/<%= dept.id %>" class="department"><%= dept.escape('name') %></a> <a href="#zones/1/stores/1/departments/<%= dept.id %>">View More <i class="icon-angle-right"></i></a></h4>
        </div>

        <ul class="items-list unstyled">
          <% dept.items().each(function (item) { %>
            <li class="aisle_513 department_82 has-details item" data-item-id="<%= item.id %>" data-department-id="<%= dept.id %>" data-aisle-id="<%= item.escape('aisle_id') %>">
              <div class="media">
                <img src="<%= item.escape('img_url') %>" alt="<%= item.escape('name') %>" style="">
                <div class="item-quantity-small">
                  <i class="icon-shopping-cart"></i>
                  <span class="item-qty">0</span>
                </div>
                <div class="item-quantity">
                  <span class="item-quantity-amount">
                    <span class="item-qty">0</span>
                  </span>
                  <span class="item-quantity-label">in cart</span>
                </div>
                <div class="info-group">
                  <div class="pull-right qty-buttons">
                    <button class="btn btn-qty btn-success dec">
                      <i class="icon-minus"></i>
                    </button>
                    <button class="btn btn-qty btn-success inc">
                      <i class="icon-plus"></i>
                      add
                    </button>
                  </div>
                </div>
              </div>
              <div class="item-info">
                <div class="item-name item-row">
                  <span class="full-item-name"><%= item.escape('name') %></span>
                  <span class="item-size muted">
                    $<%= item.escape('price') %>
                    <strong>Â·</strong>
                    <%= item.escape('size') %>
                  </span>
                </div>
              </div>
            </li>

          <% }); %>
        </ul>

      <% }); %>
    </div>
  </div>

</div>
